#include <stdio.h>
#include <stdlib.h>


int main() {
	
	int i,number, array[10],a=0;
	
	
	for(i=0;i<10;i++)
	{
		printf("Bir sayi giriniz: ");
		scanf("%d",&number);
	
		if(number!=-1)
		{
			array[i]=number;
			a+=1;
		}
				
		else 
			break;	
		
	}
	
	for(i=0;i<a+1;i++)
		printf("%d ", array[i]*array[i]);
	

	return 0;	
}
