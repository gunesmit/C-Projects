#include <stdio.h>
#include <stdlib.h>


int main() {
	
	int i;
	float a[8], toplam=0.0, ort;
	
	printf("8 tane sayi giriniz: ");
	
	for(i=0;i<8;i++)
	{
		scanf("%f",&a[i]);
		toplam+=a[i];
	}
	
	ort=toplam/8;
	
	printf("Dizinin elemanlarinin ortalamasi: %f",ort);
	
	
	
	
	return 0;
}
